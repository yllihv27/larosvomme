<p id="notice"><%= notice %></p>

<h1>Course Places</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_places.each do |course_place| %>
      <tr>
        <td><%= course_place.name %></td>
        <td><%= course_place.address %></td>
        <td><%= course_place.phone %></td>
        <td><%= course_place.description %></td>
        <td><%= link_to 'Show', course_place %></td>
        <td><%= link_to 'Edit', edit_course_place_path(course_place) %></td>
        <td><%= link_to 'Destroy', course_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course Place', new_course_place_path %>
